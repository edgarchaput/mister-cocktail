<h2><%= @cocktail.name %></h2>
  <% @cocktail.doses.each do |dose| %>
    <ul>
      <li><%= "#{dose.description} #{dose.ingredient.name}"%><p><%= link_to "Delete", dose_path(dose), method: :delete %></p></li>
    </ul>
  <% end %>
<p><%= link_to "Back to cocktails", cocktails_path, class: "btn btn-primary" %></p>



